{
    "name": "alxdorosenco/ecb-rates",
    "description": "This package uses exchange reference rates from European Central Bank",
    "keywords": [
        "ecb-rates",
        "ecb-currency-rates",
        "currency",
        "exchange",
        "laravel",
        "lumen",
        "rates",
        "European Central Bank"
    ],
    "homepage": "https://github.com/alxdorosenco/ecb-currency-rates",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexei Dorosenco",
            "email": "alexforever1808@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AlxDorosenco\\EcbRates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlxDorosenco\\EcbRates\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
